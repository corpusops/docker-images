[program:crond]
command = sh -c '\
    set -ex;if ( which cron >/dev/null 2>&1 );then cron=$(which cron); \
    elif ( which crond >/dev/null 2>&1 );then cron=$(which crond); \
    else echo "no cron";exit 1;fi \
    && exec $cron -f'
autostart = true
startsecs = 1
autorestart = true
stdout_logfile=/dev/stdout
stderr_logfile=/dev/stderr
